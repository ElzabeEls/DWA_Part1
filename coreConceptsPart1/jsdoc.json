{
    "source": {
        "include": [
            "src"
        ],
        "includePattern": "(\\.js$|docs)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "tempaltes": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts": {
        "recurse": true,
        "destination": "./docs",
        "template": "./node_modules/custom_template"
    }
}